{
  "name": "@verone/back-office",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test:e2e": "./tests/run-e2e-tests.sh",
    "test:e2e:critical": "./tests/run-e2e-tests.sh --suite critical",
    "test:e2e:performance": "./tests/run-e2e-tests.sh --suite performance",
    "test:e2e:api": "./tests/run-e2e-tests.sh --suite api",
    "test:e2e:mobile": "./tests/run-e2e-tests.sh --project mobile-workflows",
    "test:e2e:headed": "./tests/run-e2e-tests.sh --headed",
    "test:e2e:debug": "./tests/run-e2e-tests.sh --headed --debug",
    "playwright:install": "npx playwright install",
    "playwright:codegen": "npx playwright codegen localhost:3000",
    "playwright:show-report": "npx playwright show-report .playwright-mcp/reports",
    "validate:structure": "node scripts/validate-repository-structure.js",
    "fix:structure": "node scripts/auto-fix-structure.js",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "generate:stories": "node tools/scripts/generate-stories.js",
    "generate:stories:dry": "node tools/scripts/generate-stories.js --dry-run",
    "generate:stories:force": "node tools/scripts/generate-stories.js --force",
    "audit:duplicates": "jscpd src/ --min-lines 5 --min-tokens 50 --format 'console'",
    "audit:cycles": "madge --circular --extensions ts,tsx src/",
    "audit:dependencies": "depcruiser --config .dependency-cruiser.js src/ || echo 'Note: .dependency-cruiser.js config à créer'",
    "audit:deadcode": "knip",
    "audit:unused": "ts-prune",
    "audit:spelling": "cspell 'src/**/*.{ts,tsx,md}' 'docs/**/*.md' 'CLAUDE.md' 'packages/**/*.md'",
    "audit:all": "npm run audit:duplicates && npm run audit:cycles && npm run audit:deadcode && npm run audit:spelling",
    "lint:strict": "eslint . --config .eslintrc.strict.json",
    "lint:strict:fix": "eslint . --config .eslintrc.strict.json --fix",
    "validate:types": "node scripts/validation/check-db-type-alignment.ts",
    "validate:hooks": "node scripts/validation/check-duplicate-hooks.ts",
    "validate:naming": "node scripts/validation/check-naming-consistency.ts",
    "validate:all": "npm run type-check && npm run validate:types && npm run validate:hooks && npm run validate:naming",
    "prepare": "husky"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.17.1",
    "@hookform/resolvers": "^5.2.2",
    "@playwright/mcp": "^0.0.40",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@radix-ui/react-visually-hidden": "^1.2.3",
    "@react-pdf/renderer": "^4.3.1",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.57.4",
    "@tanstack/react-query": "^5.20.1",
    "@tanstack/react-table": "^8.21.3",
    "@types/dompurify": "^3.0.5",
    "@vercel/analytics": "^1.5.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "cmdk": "^1.1.1",
    "csv-parse": "^6.1.0",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.2",
    "exceljs": "^4.4.0",
    "googleapis": "^160.0.0",
    "html2canvas": "^1.4.1",
    "isomorphic-dompurify": "^2.28.0",
    "jspdf": "^3.0.3",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.309.0",
    "next": "15.2.6",
    "process": "^0.11.10",
    "react": "18.3.1",
    "react-day-picker": "^9.10.0",
    "react-dom": "18.3.1",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.64.0",
    "react-hot-toast": "^2.4.1",
    "recharts": "^3.2.1",
    "sonner": "^2.0.7",
    "swr": "^2.3.6",
    "tailwind-merge": "^2.2.1",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
    "@playwright/test": "^1.55.0",
    "@storybook/addon-a11y": "^9.1.13",
    "@storybook/addon-docs": "^9.1.13",
    "@storybook/addon-onboarding": "^9.1.13",
    "@storybook/addon-vitest": "^9.1.13",
    "@storybook/nextjs": "^9.1.13",
    "@types/node": "^20.11.17",
    "@types/react": "^18.2.55",
    "autoprefixer": "^10.4.17",
    "cspell": "^9.2.1",
    "csv-parser": "^3.2.0",
    "dependency-cruiser": "^17.1.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "^15.0.3",
    "eslint-plugin-storybook": "^9.1.13",
    "husky": "^9.1.7",
    "jscpd": "^4.0.5",
    "knip": "^5.66.2",
    "lint-staged": "^16.2.6",
    "madge": "^8.0.0",
    "playwright": "^1.55.0",
    "postcss": "^8.4.35",
    "prettier": "^3.6.2",
    "storybook": "^9.1.13",
    "tailwindcss": "^3.4.1",
    "ts-prune": "^0.10.3",
    "tsx": "^4.20.6",
    "typescript": "^5.3.3"
  }
}
